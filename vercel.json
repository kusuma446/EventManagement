{

    "version": 2,
    "build": [
        {
            "src": "dist/index.js",
            "use": "@versel/node",
            "config":{
                "includeFiles": ["dist/**"]
            }
        }
    ],
    "router": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}